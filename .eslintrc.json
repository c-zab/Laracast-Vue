{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended"
    ],
    "rules": {
		"indent": [2, "tab"],
		"no-console": "off",
		"no-tabs": "off",
		"comma-dangle": 0
	},
    "parserOptions": {
		"emacVersion": 6,
		"parser": "babel-eslint"
    }
}

