{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6,
		"sourceType": "module",
		"parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
		"indent": [2, "tab"],
		"no-console": "off"
    }
}
